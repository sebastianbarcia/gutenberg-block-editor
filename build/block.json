{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/boton-bloque",
  "version": "0.1.0",
  "title": "Boton Bloque",
  "category": "widgets",
  "icon": "smiley",
  "description": "Example block scaffolded with Create Block tool.",
  "example": {},
  "supports": {
    "html": false
  },
  "textdomain": "boton-bloque",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js",
  "attributes": {
    "content": {
      "type": "string",
      "source": "html",
      "selector": "p"
    },
    "align": {
      "type": "string",
      "default": "none"
    },
    "backgroundColor": {
      "type": "string"
    },
    "textColor": {
      "type": "string"
    },
    "kaLink": {
      "type": "string",
      "default": ""
    },
    "linkLabel": {
      "type": "string",
      "default": "Check it out!"
    },
    "hasLinkNofollow": {
      "type": "boolean",
      "default": false
    }
  },
  "styles": [
    {
      "name": "default",
      "label": "Default",
      "isDefault": true
    },
    {
      "name": "border",
      "label": "Border"
    },
    {
      "name": "color-red",
      "label": "ColorRed"
    }
  ],
  "keywords": [
    "bloque",
    "boton",
    "click"
  ]
}